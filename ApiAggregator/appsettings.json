{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenWeatherMap": {
    "BaseUrl": "https://api.openweathermap.org/data/2.5/",
    "ApiKey": "{YOUR_API_KEY}"
  },
  "NewsAPI": {
    "BaseUrl": "https://newsapi.org/v2/",
    "ApiKey": "{YOUR_API_KEY}"
  },
  "GitHubAPI": {
    "BaseUrl": "https://api.github.com",
    "AccessToken": "{YOUR_ACCESS_TOKEN}"
  }
}
